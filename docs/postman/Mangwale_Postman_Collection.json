							{"key": "moduleId", "value": "{{moduleId}}", "type": "text"}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"cart\": [{\"item_id\": \"1\", \"quantity\": 2}],\n  \"payment_method\": \"cash_on_delivery\",\n  \"order_type\": \"delivery\",\n  \"store_id\": \"{{store_id}}\",\n  \"distance\": 5.5,\n  \"address\": \"123 Main St\",\n  \"longitude\": \"0.0\",\n  \"latitude\": \"0.0\",\n  \"coupon_code\": \"\",\n  \"order_note\": \"\",\n  \"dm_tips\": 0\n}"
